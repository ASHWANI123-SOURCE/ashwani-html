<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

 
 <hibernate-mapping>
 	<class name = "com.budhh.hibernate.AlertSubs" table ="alert_subs">
 		<id name="id" column ="id" type ="java.lang.Long"></id>
 		<property name="ani" column ="ani" type ="java.lang.Long"/>
 		<property name="status" column ="status" type ="java.lang.Integer"/>
 		<property name="subDate" column ="sub_date" type ="java.sql.Timestamp"/> 
 		<property name="lastBillDate" column ="last_bill_date" type ="java.sql.Timestamp"/>
 		<property name="renewDate" column ="renew_date" type ="java.sql.Timestamp"/>
 		<property name="sub_type" column ="sub_type" type ="java.lang.String"/>
 			
 	</class>

 </hibernate-mapping>